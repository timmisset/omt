{
  "name": "Param",
  "shortcut": {
    "regEx": {
      "pattern": "^\\s*(\\$\\w+)\\s*\\(([^)]+)\\)?\\s*$",
      "assignTo": [
        "name",
        "type"
      ]
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "node": "scalar",
      "required": true
    },
    "type": {
      "type": "string",
      "required": false
    }
  }
}
